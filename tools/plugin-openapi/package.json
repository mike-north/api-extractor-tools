{
  "name": "@api-extractor-tools/plugin-openapi",
  "version": "0.1.0-alpha.2",
  "description": "OpenAPI v2/v3 input processor plugin for change-detector (isomorphic - works in Node.js and browsers)",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "pnpm inject-version && pnpm build:esm && pnpm build:cjs",
    "inject-version": "node scripts/inject-version.mjs",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "test": "vitest run && pnpm test:types",
    "test:types": "tsd --files 'test/**/*.test-d.ts'",
    "test:coverage": "vitest run --coverage",
    "check": "pnpm check:eslint && pnpm check:typecheck-tests",
    "check:eslint": "eslint src test",
    "check:typecheck-tests": "tsc -p test/tsconfig.json"
  },
  "keywords": [
    "openapi",
    "swagger",
    "openapi-v2",
    "openapi-v3",
    "api",
    "change-detection",
    "plugin",
    "isomorphic",
    "browser",
    "change-detector:plugin",
    "change-detector:input-processor-plugin"
  ],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.24.0",
  "dependencies": {
    "@api-extractor-tools/change-detector-core": "workspace:*",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^25.0.3",
    "@vitest/coverage-v8": "^4.0.15",
    "pkg-up": "^5.0.0",
    "tsd": "^0.33.0",
    "vitest": "^4.0.15"
  },
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false
}
