{
  "name": "@api-extractor-tools/change-detector",
  "version": "0.0.1",
  "private": true,
  "description": "Detects changes between two API Extractor rollup files",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "change-detector": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "generate:api-report": "api-extractor run --local --verbose",
    "test": "vitest run",
    "check": "pnpm check:eslint && pnpm check:typecheck-tests && pnpm check:api-report",
    "check:eslint": "eslint src",
    "check:typecheck-tests": "tsc -p test/tsconfig.json",
    "check:api-report": "api-extractor run"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.24.0",
  "dependencies": {
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.55.1",
    "@types/node": "^22.10.2",
    "fixturify-project": "^7.1.3",
    "vitest": "^4.0.15"
  }
}
