{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "workspaces": {
    ".": {
      "ignoreBinaries": ["changeset-change-detector"]
    },
    "lib/*": {
      "entry": ["src/index.ts"],
      "paths": {
        "@": ["./src/index.ts"]
      }
    },
    "tools/*": {
      "entry": ["src/index.ts"],
      "paths": {
        "@": ["./src/index.ts"]
      }
    }
  }
}
